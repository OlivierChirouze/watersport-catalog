{
  "name": "watersport-catalog",
  "version": "0.1.0",
  "author": "olivier.chirouze@gmail.com",
  "homepage": "",
  "main": "dist/out-tsc/index.js",
  "types": "dist/out-tsc/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint",
    "format:source": "prettier --write \"src/**/*.ts\"",
    "format:products": "prettier --write \"products/*.json\"",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format:source && git add -A src products",
    "postversion": "git push && git push --tags",
    "generate:patrik": "ts-node src/brands/patrik.ts && npm run format:products",
    "generate:gaastra": "ts-node src/brands/gaastra.ts && npm run format:products",
    "generate:exocet": "ts-node src/brands/exocet.ts && npm run format:products",
    "generate:fanatic": "ts-node src/brands/fanatic.ts && npm run format:products",
    "generate:point-7": "ts-node src/brands/point-7.ts && npm run format:products"
  },
  "peerDependencies": {
    "ts-lib": "0.0.5"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@types/puppeteer": "^5.4.4",
    "eslint": "^7.13.0",
    "jest": "^27.0.6",
    "prettier": "^1.19.1",
    "puppeteer": "^10.1.0",
    "scrape-it": "^5.3.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.1.0",
    "typescript": "~4.0"
  },
  "description": "Watersport catalog",
  "browserslist": [
    "> 5%"
  ]
}

{
  "name": "watersport-catalog",
  "version": "0.0.7",
  "author": "olivier.chirouze@gmail.com",
  "homepage": "",
  "main": "dist/out-tsc/index.js",
  "types": "dist/out-tsc/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.ts\" \"products/*.json\"",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src example",
    "postversion": "git push && git push --tags",
    "scrape:patrik": "ts-node src/brands/patrik.ts",
    "scrape:gaastra": "ts-node src/brands/gaastra.ts",
    "scrape:exocet": "ts-node src/brands/exocet.ts"
  },
  "peerDependencies": {
    "ts-lib": "0.0.5"
  },
  "dependencies": {},
  "devDependencies": {
    "puppeteer": "^10.1.0",
    "scrape-it": "^5.3.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@types/puppeteer": "^5.4.4",
    "eslint": "^7.13.0",
    "jest": "^27.0.6",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^7.0.1",
    "typescript": "~4.0"
  },
  "description": "Watersport catalog",
  "browserslist": [
    "> 5%"
  ]
}
